{
    "root": true,
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "extends": [
        "eslint:recommended"
    ],
    "rules": {
        "no-console": 1,
        "no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "^_"
            }
        ]
    },
    "env": {
        "es6": true,
        "node": true,
        "jasmine": true
    }
}